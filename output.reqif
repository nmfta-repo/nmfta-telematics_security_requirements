        <SPEC-OBJECT IDENTIFIER="SECTION-58b57f39-76db-4141-8f72-43e44079c654">
          <VALUES>
            <ATTRIBUTE-VALUE-STRING THE-VALUE="Cybersecurity Requirements for Telematics Systems Matrix Description">
              <DEFINITION>
                <ATTRIBUTE-DEFINITION-STRING-REF>ReqIF.ChapterName</ATTRIBUTE-DEFINITION-STRING-REF>
              </DEFINITION>
            </ATTRIBUTE-VALUE-STRING>
          </VALUES>
