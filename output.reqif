        <SPEC-OBJECT IDENTIFIER="REQUIREMENT-f77168cf-621f-4364-b8bb-2e4dfcfabb7f">
          <VALUES>
            <ATTRIBUTE-VALUE-STRING THE-VALUE="SII-180">
              <DEFINITION>
